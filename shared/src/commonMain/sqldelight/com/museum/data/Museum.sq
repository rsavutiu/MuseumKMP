CREATE TABLE IF NOT EXISTS Museum (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    location TEXT,
    description TEXT,
    imageUrl TEXT
);

selectAll:
SELECT * FROM Museum;

selectById:
SELECT * FROM Museum WHERE id = ?;

insertMuseum:
INSERT INTO Museum(name, location, description, imageUrl)
VALUES (?, ?, ?, ?);

deleteMuseum:
DELETE FROM Museum WHERE id = ?;
